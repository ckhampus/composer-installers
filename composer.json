{
  "name": "queensbridge/composer-installers",
  "type": "composer-installer",
  "autoload": {
    "psr-0": {"Queensbridge\\Composer": "src/"}
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.8",
    "composer/composer": "1.0.x-dev",
    "fabpot/php-cs-fixer": "dev-master"
  },
  "extra": {
    "class": [
      "Queensbridge\\Composer\\WordPressInstaller",
      "Queensbridge\\Composer\\CookbookInstaller"
    ]
  },
  "config": {
    "bin-dir": "bin"
  }
}